#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n,sum=0;
    cin>>n;
    while(n>0)
    {
        int ld=n%10;
        sum=sum+ld;
        n=n/10;
    }
    cout<<sum;
    return 0;
}
